[run]
source = km3io

[report]
include =
    km3io/*
omit =
    tests/*
    km3io/__version__.py
    km3io/trigger.py
    km3io/fitparameters.py
    km3io/reconstruction.py
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if self.debug:
    if settings.DEBUG
    def __repr__
